<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <corners
        android:topLeftRadius="10dp" android:topRightRadius="10dp"
        android:bottomLeftRadius="10dp" android:bottomRightRadius="10dp"/>
    <!--<stroke android:width="1dip" android:color="#61a0c0"/>-->
    <solid android:color="@color/white"/>
    <stroke android:color="@color/lightgrey" android:width="01dp"/>




    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners
                android:topLeftRadius="10dp" android:topRightRadius="10dp"
                android:bottomLeftRadius="10dp" android:bottomRightRadius="10dp"/>
            <!--<stroke android:width="1dip" android:color="#61a0c0"/>-->
            <solid android:color="@color/lightgrey"/>
            <stroke android:color="@color/lightgrey" android:width="01dp"/> </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle">
            <corners
                android:topLeftRadius="10dp" android:topRightRadius="10dp"
                android:bottomLeftRadius="10dp" android:bottomRightRadius="10dp"/>
            <!--<stroke android:width="1dip" android:color="#61a0c0"/>-->
            <solid android:color="@color/lightgrey"/>
            <stroke android:color="@color/lightgrey" android:width="01dp"/>
        </shape>
    </item>
    <!--<gradient android:angle="-90" android:startColor="#61a0c0" android:endColor="#133739" />-->
</shape>